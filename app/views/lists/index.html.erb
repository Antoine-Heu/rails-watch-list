<button class="m-2 p-2 border rounded-2 bg-dark text-white">
  <%= link_to "Home",
  movies_path(@task) %>
</button>
<div>
  <%= link_to "Create a list",
  new_list_path(@list) %>
</div>
<h2>My lists</h2>
<ul>
  <% @lists.each do |list| %>
    <li><%= link_to list.name, list_path(list) %></li>
  <% end %>
</ul>
